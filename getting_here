<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var repo = getRepoDetailsByName("Getting Here Banner").images[0];
            var banner = getImageURL(repo.photo_url);
            $(".page_header").css({backgroundImage: "url(" + banner + ")"});
            
            var prefix = get_prefix();
            var pages_json = prefix + "/pages/thegateway-getting-here.json"
            $.getJSON(pages_json).done(function(data) {
                $('#mm_page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            });
            
            show_content();
        }
        loadMallDataCached(renderAll); 
    });
</script>
                    