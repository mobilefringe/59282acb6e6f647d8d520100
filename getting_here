    <div class="row">
        <div class="col-md-12">
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3021.6672167444244!2d-111.90535884900406!3d40.76934352922433!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0xbf4b78f58640694f!2sThe+Gateway!5e0!3m2!1sen!2sca!4v1496955398259" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
            <div class="margin_30"></div>
            <div id="mm_page_content"></div>    
        </div>
    </div>
</div>
<script>
$(document).ready(function(e){
    init(e);
    function renderAll(){
        var repo = getRepoDetailsByName("Getting Here Banner");
        if (repo != undefined) {
            var banner = getImageURL(repo.images[0].photo_url);
            $(".page_header").css({backgroundImage: "url(" + banner + ")"});
        }
        
        var prefix = get_prefix();
        var pages_json = prefix + "/pages/thegateway-getting-here.json"
        $.getJSON(pages_json).done(function(data) {
            $('#mm_page_content').html(data.body);
        }).fail(function(jqXHR) {
            if (jqXHR.status == 404) {
                $("#404_msg").fadeIn("fast");
            }
        });
        
        show_content();
    }
    loadMallDataCached(renderAll); 
});
</script>
                    